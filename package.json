{
  "name": "@applitools/coverage-tests",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "index.js",
  "description": "Coverage Tests for Applitools SDKs",
  "author": "Applitools <team@applitools.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applitools/sdk.coverage.tests.git"
  },
  "scripts": {
    "ruby": "cd ruby && coverage-tests create-tests --path ./configuration && yarn ruby:files",
    "ruby:files": "cd ruby/test/coverage/generic && for file in *.spec.js; do mv \"$file\" \"${file/.spec.js/_spec.rb}\"; done",
    "ruby:run": "APPLITOOLS_BATCH_ID=$(uuidgen) cd ruby && rspec --default-path ./test test/coverage/generic",
    "ruby:conf:test": "mocha './ruby/configuration/test/*.spec.js'",
    "python": "cd python && coverage-tests create-tests --path ."
  },
  "dependencies": {
    "@applitools/sdk-coverage-tests": "1.0.3",
    "@typescript-eslint/parser": "^2.14.0",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "mocha": "^8.0.1"
  }
}
